{
  "name": "@demvsystems/fuzzy-date",
  "version": "1.0.0",
  "description": "Fuzzy date parser for easy user input interpretation",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/demvsystems/fuzzy-date"
  },
  "engines": {
    "node": ">=6",
    "npm": ">=3"
  },
  "scripts": {
    "build:clean": "rimraf dist",
    "build:transpile": "babel --ignore '**/*.spec.js' -d dist src",
    "build": "run-s build:*",
    "prepublishOnly": "run-s build",
    "lint": "eslint --ignore-path .gitignore .",
    "start": "cross-env NODE_ENV=development jest --watch --notify",
    "test": "jest"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "keywords": [
    "fuzzy",
    "date",
    "parse",
    "format"
  ],
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5",
    "@babel/preset-env": "^7.1.5",
    "@babel/register": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "codecov": "^3.1.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.8.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-unicorn": "^7.0.0",
    "husky": "^1.1.3",
    "jest": "^23.6.0",
    "lint-staged": "^8.0.4",
    "lodash": "^4.17.11",
    "npm-run-all": "^4.1.3",
    "rimraf": "^2.6.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint",
      "jest --bail --findRelatedTests"
    ]
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "0.10"
          }
        }
      ]
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "coverageReporters": [
      "lcov",
      "text",
      "html"
    ]
  },
  "eslintConfig": {
    "extends": [
      "airbnb-base",
      "plugin:unicorn/recommended"
    ],
    "env": {
      "browser": false
    },
    "overrides": [
      {
        "files": [
          "*.spec.js"
        ],
        "env": {
          "jest": true
        },
        "rules": {
          "import/no-extraneous-dependencies": [
            "error",
            {
              "devDependencies": true
            }
          ]
        }
      }
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
